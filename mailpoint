    <div class="wysija_content center" wysija_align="center" >
        <div wysija_ratio="1.000" class="wysija_image empty" >
    
        
        <img height="80" width="80" class="image_placeholder empty" src="https://newsletter.poli.usp.br/wp-content/plugins/wysija-newsletters/img/transparent.png" alt="" />
    

    
